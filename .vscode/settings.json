{
    "cmake.configureOnOpen": true,
    "C_Cpp.default.includePath": [
        "venv/lib/python3.10/site-packages/torch/include/torch/csrc/api/include/torch",
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/venv": true,
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.tabSize": 4,
    },
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "files.associations": {
        "functional": "cpp"
    }
}